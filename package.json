{
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/picomatch": "^2.3.0"
  },
  "optionalDependencies": {
    "cypress": "^12.17.4",
    "netlify-cli": "^16.0.3"
  },
  "scripts": {
    "build": "yarn run build:backend && yarn run build:frontend",
    "build:backend": "yarn run build:netlify",
    "build:frontend": "webpack --config ./webpack.frontend.js",
    "build:netlify": "webpack --config ./webpack.netlify.functions.js",
    "build:style": "webpack --config ./webpack.style.js",
    "fix": "eslint . --fix && prettier . --write",
    "lint": "eslint .",
    "posttest": "rimraf ./globalConfig.json",
    "prebuild": "rimraf ./functions && rimraf ./dist/js && mkdir -p ./dist/js",
    "start:db": "sudo systemctl start mongod",
    "start:backend": "netlify dev --port 7070",
    "start:frontend": "webpack-dev-server --config ./webpack.frontend.js",
    "start": "NODE_ENV=\"development\" NODE_OPTIONS=\"--max-old-space-size=4096\" netlify dev --port 7070 --command \"yarn run start:frontend\"",
    "test": "yarn test:backend && yarn test:frontend",
    "test:backend": "jest --config jest.backend.config.ts",
    "test:cypress": "cypress run",
    "test:frontend": "jest --config jest.frontend.config.ts",
    "watch:backend": "tsc -p tsconfig.netlify.functions.json --listEmittedFiles --watch",
    "watch:frontend": "webpack --watch",
    "watch:lint": "esw -w ./src/**/*.ts --color --clear --changed",
    "watch:test": "jest --watch --noStackTrace"
  },
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@netlify/functions": "^1.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@tsconfig/svelte": "^5.0.0",
    "@types/aws-lambda": "^8.10.95",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@xstate/cli": "^0.5.15",
    "@xstate/svelte": "^2.1.0",
    "@xstate/test": "^1.0.0-alpha.1",
    "babel-jest": "^29.7.0",
    "bcryptjs": "^2.4.3",
    "carbon-icons-svelte": "^12.4.2",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-watch": "^8.0.0",
    "http-server": "^14.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonwebtoken": "^9.0.1",
    "merge": "^2.1.1",
    "mini-css-extract-plugin": "^2.7.6",
    "mongodb": "^5.8.0",
    "mongodb-client-encryption": "^2.3.0",
    "mongodb-memory-server": "^8.5.1",
    "normalize-url": "^8.0.0",
    "picomatch": "^2.3.1",
    "prando": "^6.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "^2.10.1",
    "rimraf": "^3.0.2",
    "saslprep": "^1.0.3",
    "sass": "^1.69.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "svelte": "^3.0.0",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess": "5.1.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.1.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.15.1",
    "webpack-license-plugin": "^4.4.2",
    "xstate": "^4.38.1",
    "yarn": "^1.22.21"
  },
  "resolutions": {
    "@cypress/request": "^3.0.0",
    "async": ">2.6.4 || ^2.6.4",
    "glob-parent": ">6.0.2 || ^6.0.2"
  },
  "documentation": {
    "resolutions": {
      "@cypress/request": "Resolved to version 3.0.0 to address security vulnerability. q.v. https://github.com/advisories/GHSA-p8p7-x288-28g6"
    }
  }
}
